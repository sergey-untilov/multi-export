{
  "name": "multi-export",
  "version": "0.1.0",
  "description": "Multi export for import into A5",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "rmad": "rm -f -r %APPDATA%/multi-export",
    "test": "jest"
  },
  "repository": "https://github.com/sergey-untilov/multi-export",
  "keywords": [
    "Electron",
    "export",
    "A5"
  ],
  "author": "UncleZeppelin",
  "license": "MIT",
  "build": {
    "appId": "multi-export",
    "win": {
      "target": "portable",
      "icon": "build/multi-export.ico"
    },
    "directories": {
      "buildResources": "build"
    },
    "extraFiles": [
      {
        "from": "assets",
        "to": "assets"
      }
    ]
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "jest": "^24.9.0",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "electron-reload": "^1.5.0",
    "electron-store": "^5.1.0",
    "iconv": "^2.3.5",
    "mssql": "^6.0.1",
    "spectre.css": "^0.5.8"
  }
}
